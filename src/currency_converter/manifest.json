{
  "manifest_version": 2,

  "name": "Currency Converter",
  "description": "This extension allows the user to convert any currencies on a given page to another currency",
  "version": "1.0",
  "browser_action": {
    "default_icon": "./img/icon16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/img/icon16.png",
    "32": "/img/icon32.png",
    "64": "/img/icon64.png",
   "128": "/img/icon128.png"
  },
  "permissions": ["activeTab"]
}
